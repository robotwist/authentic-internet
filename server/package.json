{
  "name": "server",
  "version": "1.0.0",
  "type": "module",
  "main": "server.mjs",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node server.mjs",
    "dev": "nodemon --watch '*.mjs' --watch 'routes/**/*.mjs' --watch 'controllers/**/*.mjs' --watch 'models/**/*.mjs' --watch 'middleware/**/*.mjs' --watch 'services/**/*.mjs' --watch 'utils/**/*.mjs' --watch 'config/**/*.mjs' --ignore 'node_modules/' --ignore '.git/' --ignore 'logs/' --ignore '*.log' --ignore '*.pid' server.mjs",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "lint": "eslint . --ext .js,.mjs",
    "lint:fix": "eslint . --ext .js,.mjs --fix"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "connect-mongo": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.0",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.14.2",
    "mongoose": "^8.2.0",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.7.4",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.4"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {},
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  }
}
